{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "noImplicitReturns": true,
    "noUnusedLocals": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "sourceMap": true,
    "resolveJsonModule": true
  },
  "include": [
    "src"
  ]
}
